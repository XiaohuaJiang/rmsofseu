<?php
App::uses('InterfaceController', 'Controller');

class MyRobotInterfaceController extends InterfaceController {

/**
 * Myrobot view action. All necessary variables are set in the main interface controller.
 *
 * @return null
 */
	public function view() {
		// set the title of the HTML page
		$this->set('title_for_layout', 'MyRobot Interface');
		// we will need some RWT libraries
		$this->set('rwt', array('roslibjs' => 'current', 'keyboardbatteryinfojs' => 'current', 'ros2djs' => 'current', 'nav2djs' => 'current', 'ros3djs' => 'current', 'nodecontroljs' => 'current'));
		
	}

}
